abstract class Car {

    String brand;
    String model;
    String type;
    String color;
    int year;
    double price;

    Car(String brand, String model, String type, String color, int year, double price) {
        this.brand = brand;
        this.model = model;
        this.type = type;
        this.color = color;
        this.year = year;
        this.price = price;
    }

    abstract void startEngine();
    abstract void stopEngine();

    void showInfo() {
        System.out.println("Brand: " + brand);
        System.out.println("Model: " + model);
        System.out.println("Type: " + type);
        System.out.println("Color: " + color);
        System.out.println("Year: " + year);
        System.out.println("Price: " + price);
    }
}

class ToyotaVios extends Car {

    ToyotaVios(String brand, String model, String type, String color, int year, double price) {
        super(brand, model, type, color, year, price);
    }

    @Override
    void startEngine() {
        System.out.println(model + " engine started using push button.");
    }

    @Override
    void stopEngine() {
        System.out.println(model + " engine stopped.");
    }

    void showSpecialFeature() {
        System.out.println("Special Feature: Keyless Entry and Push Start");
    }
}

public class Main {
    public static void main(String[] args) {
        Car myCar = new ToyotaVios("Toyota", "Vios", "Sedan", "Blue", 2024, 1200000.0);

        myCar.showInfo();
        myCar.startEngine();
        myCar.stopEngine();

        ((ToyotaVios) myCar).showSpecialFeature();
    }
}
